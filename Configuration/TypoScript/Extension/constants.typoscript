###########################
#### BOOTSTRAP PACKAGE ####
###########################
plugin.tx_bootstrappackage {
    view {
        templateRootPath = EXT:bootstrap_package/Resources/Private/Templates/
        partialRootPath = EXT:bootstrap_package/Resources/Private/Partials/
        layoutRootPath = EXT:bootstrap_package/Resources/Private/Layouts/
    }
}

#############################################################
#### EXTENSIONS                                          ####
#### Manual inclusion to keep control over loading order ####
#############################################################
[extension.isLoaded('felogin')]
    @import 'EXT:bootstrap_package/Configuration/TypoScript/Extension/Felogin/constants.typoscript'
[end]
[extension.isLoaded('form')]
    @import 'EXT:bootstrap_package/Configuration/TypoScript/Extension/Form/constants.typoscript'
[end]
[extension.isLoaded('indexed_search')]
    @import 'EXT:bootstrap_package/Configuration/TypoScript/Extension/IndexedSearch/constants.typoscript'
[end]
[extension.isLoaded('seo')]
    @import 'EXT:bootstrap_package/Configuration/TypoScript/Extension/Seo/constants.typoscript'
[end]
